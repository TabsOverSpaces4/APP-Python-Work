    "import socket\n",
    "c=socket.socket()\n",
    "host=socket.gethostname()\n",
    "port=9998\n",
    "c.connect((host,port))\n",
    "query=input(\"enter query\")\n",
    "c.send(bytes(query,'utf-8'))\n",
    "print(c.recv(1024).decode())"
